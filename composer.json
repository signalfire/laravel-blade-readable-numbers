{
    "name": "sandervanhooft/laravel-blade-readable-numbers",
    "description": "A blade directive for human readable numbers, k-style.",
    "keywords": [
        "Sander van Hooft",
        "laravel-blade-readable-numbers",
        "laravel",
        "blade",
        "human-readable"
    ],
    "homepage": "https://github.com/sandervanhooft/laravel-blade-readable-numbers",
    "license": "MIT",
    "authors": [
        {
            "name": "Sander van Hooft",
            "email": "info@sandervanhooft.com",
            "homepage": "https://www.sandervanhooft.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "^5.6",
        "php": "^8.0"
    },
    "require-dev": {
        "larapack/dd": "^1.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "SanderVanHooft\\ReadableNumbers\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SanderVanHooft\\Tests\\ReadableNumbers\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SanderVanHooft\\ReadableNumbers\\ReadableNumbersServiceProvider"
            ]
        }
    }
}
